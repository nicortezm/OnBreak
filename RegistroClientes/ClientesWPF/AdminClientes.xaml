        <TextBox x:Name="txtDireccion" HorizontalAlignment="Left" Height="40" Margin="899,310,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" FontSize="24" TextChanged="txtDireccion_TextChanged"/>
