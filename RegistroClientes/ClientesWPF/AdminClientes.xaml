        <Label x:Name="lblRazonSocial" Content="Razon Social" HorizontalAlignment="Left" Margin="745,109,0,0" VerticalAlignment="Top" FontSize="24"/>
        <Label x:Name="lblRazSocobl" Content="*" HorizontalAlignment="Left" Margin="1095,109,0,0" VerticalAlignment="Top" FontSize="28"/>

