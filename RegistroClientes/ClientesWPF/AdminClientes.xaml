        <Button x:Name="btnListarClient" Content=" Listar&#xa;Cliente" HorizontalAlignment="Left" Margin="48,471,0,0" VerticalAlignment="Top" Width="235" FontSize="22" Click="btnRegistrar_Click" Background="#FF2B78E4" Foreground="#FFFFFF" FontWeight="Bold"/>
